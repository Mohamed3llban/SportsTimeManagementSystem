                                        <textarea asp-for="Bio" class="form-control form-control-lg form-control-solid" rows="3"></textarea>
                                        <!--end::Input-->
                                    </div>
                                    <!--end::Input group-->
                                    <!--begin::Input group-->
                                    <div class="fv-row mb-10">
                                        <!--end::Label-->
                                        <div><label class="form-label">ملاحظات</label></div>
                                        <!--end::Label-->
                                        <!--begin::Input-->
                                        <textarea asp-for="Note" class="form-control form-control-lg form-control-solid" rows="3"></textarea>
                                        <!--end::Input-->
                                    </div>
                                    <!--end::Input group-->
                                    
                                    <!--begin::Input group-->
                                    <div class="row g-9 mb-8 mt-8">
                                        <div class="col-md-6 fv-row">
                                            <div class="rtl"><label class="required fs-6 fw-semibold mb-2">تاريخ الإنضمام</label></div>
                                            <!--begin::Input-->
                                            <div class="position-relative d-flex align-items-center">
                                                <!--begin::Icon-->
                                                <!--begin::Svg Icon | path: icons/duotune/general/gen014.svg-->
